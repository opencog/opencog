%% Dictionary version number is 5.3.15 (formatted as V5v3v15+)
<dictionary-version-number>: V5v3v15+;
<dictionary-locale>: RU4ru+;

<costly-null>: [[[[]]]];

<наречие-куда>: ({Xc-} & Ed-) or ({XIc+} & EId+) or ({Xt- & S-});
<наречие-откуда>: ({Xc-} & Es-) or ({XIc+} & EIs+) or Mp- or ({Xt-} & S-);
<наречие-где>: ({Xc-} & Ew-) or ({XIc+} & EIw+) or ({Xt-} & S-);
<наречие-когда>: ({Xc-} & Et-) or ({Xc+} & EIt+);
<наречие-как>: ({Xc-} & Ek-) or ({XIc+} & EIk+);
<наречие-почему>: ({Xc-} & Ep-) or ({XIc+} & EIp+);

<дополнение-о-чем>: Mp- or MVap- or MVIap+;
<дополнение-без>: Mp- or <наречие-как>;
<кроме>: ((Xc- & Mp-) or <наречие-как>) & {XIc+};

%предлоги
а-ля.ji : Ji+ & <наречие-как>;
минус.ji плюс.ji: (Ji+ & Mp-) or CO+;

близ.jg кругом.jg меж.jg между.jg напротив.jg насупротив.jg около.jg 
окрест.jg подле.jg посереди.jg посередине.jg посередь.jg посреди.jg 
посредине.jg поверх.jg позади.jg позадь.jg промеж.jg промежду.jg 
середь.jg снизу.jg спереди.jg среди.jg средь.jg сверху.jg сзади.jg
у.jg вблизи.jg вкруг.jg вне.jg внизу.jg внутри.jg вокруг.jg 
возле.jg впереди.jg выше.jg : Jg+ & <наречие-где>;
для.jg от.jg ото.jg ввиду.jg вследствие.jg ради.jg : Jg+ & <наречие-почему>;
до.jg со.jg внутрь.jg вовнутрь.jg мимо.jg обок.jg вперед.jg против.jg супротив.jg : Jg+ & <наречие-куда>;
по_обе_стороны по_другую_сторону : {Jg+} & <наречие-где>;
% пришлось сидеть снаружи храма
снаружи.jg : Jg+ & <наречие-где>;

из-под.jg из-подо.jg из-за.jg из.jg изо.jg изнутри.jg с.jg : Jgp+ & <наречие-откуда>;
из_под из_подо из_за : Jgp+ & <наречие-откуда>;

наподобие.jg ниже.jg относительно.jg поперек.jg 
посредством.jg превыше.jg наместо.jg противно.jg путем.jg
сверх.jg свыше.jg вдоль.jg вкось.jg вместо.jg
вроде.jg взамен.jg : Jg+ & <наречие-как>;
касаемо.jg касательно.jg насчет.jg : Jg+ & <дополнение-о-чем>;
без.jg безо.jg опричь.jg : Jg+ & <дополнение-без>;
кроме.jg помимо.jg : Jg+ & <кроме>;
накануне.jg после.jg прежде.jg : Jg+ & <наречие-когда>;

благодаря.jd наперекор.jd подобно.jd согласно.jd сообразно.jd соответственно.jd 
соразмерно.jd вопреки.jd : Jd+ & <наречие-как>;
к.jd ко.jd вослед.jd вслед.jd : Jd+ & <наречие-куда>;
наперерез.jd навстречу.jd вдогонку.jd  : Jd+ & <наречие-куда>;

исключая.jv включая.jv выключая.jv : Jv+ & <кроме>;
на.jv сквозь.jv через.jv чрез.jv : Jv+ & <наречие-куда>;
про.jv : Jv+ & <дополнение-о-чем>;
спустя.jv под.jv : Jv+ & <наречие-когда>;
в.jv во.jv : Jv+ & <наречие-где>;
за.jv : Jv+ & <наречие-почему>;
