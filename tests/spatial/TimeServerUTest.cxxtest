        timeServer().getTimeInfo(back_inserter(res), Handle::UNDEFINED);
        TS_ASSERT(res.size() == 1);
        TS_ASSERT(res.front().getHandle() == node2);
        TS_ASSERT(*(res.front().getTemporal()) == Temporal(1000));

