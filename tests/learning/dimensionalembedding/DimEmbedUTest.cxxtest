class DimEmbedTestSuite : public CxxTest::TestSuite
{
public:
    void link(AtomSpace* as, Handle h1, Handle h2,
              double strength, double confidence)
    {
        Handle h = as->add_link(SIMILARITY_LINK, h1, h2);
        h->merge(SimpleTruthValue::createTV(strength, confidence));
    }

    void inhLink(AtomSpace* as, Handle h1, Handle h2,
                 double strength, double confidence)
    {
        Handle h = as->add_link(INHERITANCE_LINK, h1, h2);
        h->merge(SimpleTruthValue::createTV(strength, confidence));
    }

    void testMisc()
    {
        CogServer& cs = cogserver();
        AtomSpace* atomSpace = &cs.getAtomSpace();
        atomSpace->clear();
        DimEmbedModule dimEmbed = DimEmbedModule(cs);
        Handle h1 = atomSpace->add_node(CONCEPT_NODE, "dog1");
        h1->merge(SimpleTruthValue::createTV(0.001f, 0.00001f));
        Handle h2 = atomSpace->add_node(CONCEPT_NODE, "dog2");
        h2->merge(SimpleTruthValue::createTV(0.001f, 0.00001f));
        Handle h3 = atomSpace->add_node(CONCEPT_NODE, "dog3");
